<?xml version="1.0" encoding="utf-8"?>
<ripple
        xmlns:android="http://schemas.android.com/apk/res/android"
        android:color="@color/colorAccent">
    <item
            android:id="@+id/masked">
        <shape
                android:shape="rectangle">
            <solid
                    android:color="@color/colorWhite"/>

            <stroke
                android:width="1dp"
                android:color="@color/colorAccent"/>

            <corners
                    android:radius="25dp"/>

        </shape>
    </item>

</ripple>